    <!-- Make sure session data is inside a container and positioned correctly -->
    <div id="sessionDataContainer">
        <!-- This is where the session data will be displayed -->
        <div id="sessionData"></div>
      </div>

<script>
  // Access the session data passed from the server and inject it into JavaScript
  const sessionData = <%- JSON.stringify(sessionData) %>;  // Use raw output to inject the session data into JavaScript

  // Access roomType from sessionData
  const roomType = sessionData.reservation.roomType;

  // Print the roomType value inside the div with id="sessionData"
  document.getElementById('sessionData').textContent = roomType;
  
  console.log(sessionData);  // Log entire session data in the console (for debugging)
</script>

